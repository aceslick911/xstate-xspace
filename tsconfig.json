{
  "compilerOptions": {
    "lib": ["dom"],
    "strict": false,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "moduleResolution": "node",
    "noEmit": true,
    "target": "esnext",
    "noImplicitAny": false,
    "strictNullChecks": false,
    "skipLibCheck": true,
    "keyofStringsOnly": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js",
    "__mocks__"
  ]
}
